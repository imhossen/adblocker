{
    "name": "Ad Blocker",
    "description": "Ad Blocker mady by Hossen",
    "manifest_version": 2,
    "version": "1.0",
    "background": {
        "scripts": [
            "blockedsites.js",
            "background.js"
        ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "http://*/",
        "https://*/"
    ],
    "icons": {
        "16": "icons/logo_16x16.png",
        "48": "icons/logo_48x48.png",
        "128": "icons/logo_128x128.png"
    }
}